{
  "name": "yarn-plugin-unused-resolutions",
  "description": "A Yarn plugin that checks for unused resolutions in package.json",
  "version": "0.1.2",
  "private": true,
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && builder build plugin",
    "build:debug": "builder build plugin --source-map --no-minify --metafile",
    "clean": "rimraf bundles",
    "format": "prettier --write .",
    "test": "node test.js",
    "postinstall": "yarn husky"
  },
  "dependencies": {
    "@yarnpkg/cli": "^4.12.0",
    "@yarnpkg/core": "^4.5.0",
    "@yarnpkg/fslib": "^3.1.4",
    "clipanion": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@yarnpkg/builder": "^4.2.3",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@yarnpkg/core/tar": "7.5.7"
  },
  "packageManager": "yarn@4.12.0"
}
